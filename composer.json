{
  "name": "phpacadem/framework",
  "description": "Psr framework",
  "type": "library",
  "keywords": ["framework","psr","router","SOLID", "component"],
  "license": "MIT",
  "authors": [
    {
      "name": "phpacadem",
      "email": "phpacadem@inbox.ru"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "filp/whoops": "^2.3",
    "league/plates": "^3.3",
    "league/route": "^4.2",
    "monolog/monolog": "^1.23",
    "php-di/php-di": "^6.0",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/log": "^1.0",
    "symfony/console": "^4.1",
    "zendframework/zend-config-aggregator": "^1.1",
    "zendframework/zend-diactoros": "^2.0",
    "zendframework/zend-httphandlerrunner": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PhpAcadem\\framework\\": "src/",
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "symfony/var-dumper": "^4.1"
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
